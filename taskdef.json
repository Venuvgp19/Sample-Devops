{
    "executionRoleArn": "arn:aws:iam::430544893156:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "Hello-World",
            "image": "430544893156.dkr.ecr.ap-south-1.amazonaws.com/helloworld:latest",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "networkMode": "awsvpc",
    "cpu": "1",
    "memory": "128",
    "family": "Hello-world"
}
